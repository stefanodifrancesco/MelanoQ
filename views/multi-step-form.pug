extends layout

block content
  div.progressline
    // progressbar
    ul#progressbar
        li.active DEMOGRAPHIC
        li PHENOTYPE
        li ULTRAVIOLET EXPOSURE
        li LIFESTYLE HABITS
        li CLINICAL EXAMINATION
  div.form
    // multistep form
    form#msform(action="/stepform" method="POST" autocomplete="off")
    
        fieldset
          div.scrollingdiv(id='scrollerdiv')
            h2.fs-title GENERAL
            h3.fs-subtitle Completed by phisician

            label(for='1237444630') Subject
            select#1237444630.form-control(name='subject')
              option(value='No Choosed' selected='') Select Subject ...
              option(value='Case') Case
              option(value='Control') Control
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldsetCodeNumber
              label(for='593911901') Database Code Number
              .divTable
                .divTableBody
                  .divTableRow
                    .divTableCell(style='border: 0px solid white !important;')
                      .form-group
                        select#DatabaseCodeCountry.form-control(name='codeCountry')
                          option(value='No Choosed' selected='') Select Country ...
                          option(value='Greece') Greece
                          option(value='Italy') Italy
                          option(value='Spain') Spain
                    .divTableCell(style='border: 0px solid white !important;')
                      .form-group.hidden-control
                        select#DatabaseCodeCenter.form-control(name='codeCenter')
                          // <option value="No Choosed" selected>Select Center ...</option>
                    .divTableCell(style='border: 0px solid white !important;')
                      .form-group
                        select#DatabaseCodeType.form-control(name='codeType')
                          option(value='No Choosed' selected='') Select case type ...
                          option(value='M') Melanoma Case
                          option(value='C') Control subject
                  .divTableRow
                    .divTableCell(style='border: 0px solid white !important;')
                      .form-group
                        input#codeNumber(type='text' readonly='' value='' name='codeNumber')

            label(for='1763927487') Date of questionnaire administration and phenotyping
            input#1763927487.datepicker.form-control(type='text' placeholder='Select date' required='' name='surveyDate')
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='9069469') Dates of updated questionnaire and/or phenotyping
            input#1969236884_date.datepicker.form-control(type='text' placeholder='Select date' required='' name='updatedInDate')
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='type_of_melanoma') Type of melanoma
            select#Melanoma.form-control(name='melanoma_type')
              option(value='No Choosed' selected='') Select Type ...
              option(value='Sporadic') Sporadic
              option(value='Familial') Familial
              option(value="Don't know") Don&apos;t know
              option(value='Other') Other (complex family history), specify:
            input#Other_Melanoma.hidden-control.inputMelanoma(type='text' name="other_melanoma_type")
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

          input.start(type='button', name='next', value='CONFIRM')

        fieldset
          div.scrollingdiv(id='scrollerdiv')
            h2.fs-title GENERAL
            h3.fs-subtitle Completed by phisician

            legend(for='1237444630') Click start button to start questionnaire
            
          input.start(type='button', name='next', value='START')

        fieldset
          div.scrollingdiv(id='scrollerdiv')
            h2.fs-title DEMOGRAPHIC
            h3.fs-subtitle Completed by case/control

            label(for='1350866299') Sex
            select#1350866299.form-control(name='gender' required='required' data-rule-required='true' data-msg-required='Please select your gender')
                option(value='' selected='') Select Gender...
                option(value='Female') Female
                option(value='Male') Male
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='2126036184_date') Date and place of birth
            input#2126036184_date.form-control.datepicker(type='text' name="birthdate"  required='required' data-rule-required='true' data-msg-required='Please include your birthday')
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='weight_input') Weight(Kg)
            input#weight_input.form-control(type='number' name='weight' required='required' data-rule-required='true' data-msg-required='Please include your weight')
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='height_input') Height(Cm)
            input#height_input.form-control(type='number' name='height' required='required' data-rule-required='true' data-msg-required='Please include your height')
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='ethnicity_input') Ethnicity
            select#ethnicity_input.form-control(name='ethnicity' required='required' data-rule-required='true' data-msg-required='Please include your ethnicity')
                option(value='' selected='') Select Ethnicity ...
                option(value='White/Europe') White/Europe
                option(value='White/NorthAfrica') White/NorthAfrica
                option(value='White/MiddleEast') White/MiddleEast
                option(value='White/Jewish') White/Jewish
                option(value='Black or African American') Black or African American
                option(value='Asian') Asian
                option(value='Hispanic or Latino (i.e. from South or Central America)')
                    | Hispanic or Latino (i.e.
                    | from
                    | South or Central America)
                option(value='Other') Other (Specify)
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle
            input#OtherEthnicity.hidden-control.inputEthnicity(type='text' name="other_ethnicity")
            

            label Residency
            #myForm
                input#residency.form-control(type='text' data-geocomplete='street address' name='residency'  required='required' data-rule-required='true' data-msg-required='Please include your residency')
                span.error1(style='display: none;')
                    i.error-log.fa.fa-exclamation-triangle
                .residency_date_class
            #myMap
            #myJsonTab
            .table            

            label(for='education_input') Education
            select#education_input.form-control(name='education' required='required' data-rule-required='true' data-msg-required='Please include your education')
                option(value='' selected='') Select category...
                option(value='Up to junior high school (up to 14-16 yrs)')
                    | Up to junior high school (up to 14-16
                    | yrs)
                option(value='High school (up to 18-19 yrs)') High school (up to 18-19 yrs)
                option(value='University') University
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='current_occupational_status') Current occupational status
            select#current_occupational_status.form-control(name='occupational_status' required='required' data-rule-required='true' data-msg-required='Please include your status')
                option(value='' selected='') Select Status...
                option(value='Employed') Employed
                option(value='Working at home') Working at home
                option(value='Unemployed') Unemployed
                option(value='Student') Student
                option(value='Retired') Retired
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldHistory
              label(for='History', style='font-size: 12px;')
                | History of occupation(optional History of occupation
                | (starting from most recent and
                | going backwards in time record duration from YYYY to YYYY)
              .form-group
                select#History.form-control(name='entry.History')
              .form-group.dynamic_ddl
              .form-group.div_date
              .table

          div.alphaformfooter
          div.formfooter
          input.scroll-button-bottom(type='button')
          input.next.action-button(type='button', name='next', value='Next')


        fieldset
          div.scrollingdiv
            h2.fs-title PHENOTYPE
            h3.fs-subtitle Completed by case/control

            label(for='skin-response') Which of the following sentences does better describe your skin response to sun exposure at noon for 30 minutes in the first days of the summer without sunscreen/clothing protection?
            select#skin-response.form-control(name='skin_response' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='') Select Skin Response ...
              option(value='Not burn') Not burn
              option(value='Burns lightly') Burns lightly
              option(value='Burns moderately') Burns moderately
              option(value='Burns severely') Burns severely
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='skin-tan') Does your skin tan after a prolonged and repeated sun exposure (e.g. after several days as at the end of the summer) without protecting it with sunscreen or clothing?
            select#skin-tan.form-control.skin-tan(name='skin_tan' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='') Select Tan...
              option(value='No tan') No tan
              option(value='Tans lightly') Tans lightly
              option(value='Tans moderately') Tans moderately
              option(value='Tans deeply') Tans deeply
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='1304821978') Eye Color
            select#id_eyes_color.form-control(name='eye_color' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='') Select eye color...
              option(value='Light') Light (green, blue, gray)
              option(value='Medium') Medium (light brown, hazel)
              option(value='Dark') Dark (dark brown, black)
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldsetHair
            label(for='1275694163') Hair color (at age of 18 yrs)
            .form-group
              .divTable
                .divTableBody
                  .divTableRow
                    .divTableCell
                      span Red Hair Color
                      input#Red_Hair(type='radio' name='entry.Red_Color' value='red')
                    .divTableCell
                      span Blonde Hair Color
                      input#Blonde_Hair(type='radio' name='entry.Blonde_Color' value='blonde')
                    .divTableCell
                      span Light Brown Color
                      input#Light_brown_Hair(type='radio' name='entry.Light_Brown_Color' value='Light brown')
                    .divTableCell
                      span Dark Brown Color
                      input#Dark_brown_Hair(type='radio' name='entry.Dark_brown' value='Dark brown')
                    .divTableCell
                      span Dark Color
                      input#Dark_Hair(type='radio' name='entry.Dark_Color' value='red')
                  .divTableRow
                    .divTableCell
                      img(src='img/Color-hair/Red_hair.png' width='150' height='50')
                    .divTableCell
                      img(src='img/Color-hair/Blonde_hair.png' width='150' height='50')
                    .divTableCell
                      img(src='img/Color-hair/Light_brown_hair.png' width='100' height='50' )
                    .divTableCell
                      img(src='img/Color-hair/Dark_brown_hair.png' width='150' height='50')
                    .divTableCell
                      img(src='img/Color-hair/Dark_hair.png' width='150' height='50')
            .form-group.hidden-control
              img#hair-img(src='' alt='' height='auto' width='auto')

            label(for='id_freckles') FrecklesDefinition: Light brown small macules since childhood, on the face, during summer time (disappear in winter)
            select#id_freckles.form-control(name='freckles' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='') Select freckles frequency ...
              option(value='None') None
              option(value='Few') Few
              option(value='Some') Some
              option(value='Many') Many
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            label(for='id_Nevi') Nevi frequency
            select#id_Nevi.form-control(name='nevi' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='') Select Nevi frequency ...
              option(value='None') None
              option(value='Few') Few
              option(value='Some') Some
              option(value='Many') Many
            span.error1(style='display: none;')
                i.error-log.fa.fa-exclamation-triangle

            
          div.alphaformfooter
          div.formfooter
          input.scroll-button-bottom(type='button')
          input.previous.action-button(type='button', name='previous', value='Previous')
          input.next.action-button(type='button', name='next', value='Next')

        fieldset
          div.scrollingdiv
            h2.fs-title HISTORY OF ULTRAVIOLET EXPOSURE
            h3.fs-subtitle Completed by case/control

            label(for='id_UltravioletExposure') Occupational sun exposure(i.e. outdoor occupation with at least 4 hours/day spent outdoor in the sun)
            select#id_UltravioletExposure.form-control(name='occupational_sun_exposure' required='required' data-rule-required='true' data-msg-required='Required field')
              option(value='No') No
              option(value='Yes') Yes
            .hidden-control(style='margin-top:20px;')
                label Specify the occupation:
                input#UltravioletExposure_Yes.form-control(type='text' name='sun_exposure_occupation')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/day?:
                input#UltravioletExposure_hours_for_day.form-control(type='text' name='sun_hour_day')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/month?:
                input#UltravioletExposure_hours_for_month.form-control(type='text' name='sun_hour_month')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/year?:
                input#UltravioletExposure_hours_for_year.form-control(type='text' name='sun_hour_year')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many years ?:
                input#UltravioletExposure_many_years.form-control(type='text' name='sun_num_years')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle

            label(for='id_RecreationalExposure') Recreational sun exposure (i.e. outdoor hobbies or physical activities&ndash;other than sunbathing&ndash;with at least 4 hours/day spent outdoor in the sun)
            select#id_RecreationalExposure.form-control(name='recreational_exposure' required='required' data-rule-required='true' data-msg-required='Required field')
                option(value='No') No
                option(value='Yes') Yes
            .hidden-control(style='margin-top:20px;')
                label Specify the occupation:
                input#RecreationalExposure_Yes(type='text' name='recreational_exposure_occupation')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/day?:
                input#Recreational_hours_for_day(type='text' name='recreational_exposure_hour_day')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/month?:
                input#Recreational_hours_for_month(type='text' name='recreational_exposure_hour_month')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many hours/year?:
                input#Recreational_hours_for_year(type='text' name='recreational_exposure_hour_year')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
                label How many years ?:
                input#Recreational_many_years(type='text' name='recreational_exposure_num_years')
                span.error1(style='display: none;')
                  i.error-log.fa.fa-exclamation-triangle
              
            label Intermittent sun exposure (i.e. sun exposure in spring and summer time, including vacation, during peak hours&ndash;11AM-4PM)
            .form-group
              .divTable
                .divTableBody
                  .divTableRow
                    .divTableCell Age Period
                    .divTableCell Exposure Period
                    .divTableCell Exposure Time
                  .divTableRow
                    .divTableCell
                    .divTableCell Weeks of vacation
                    .divTableCell Hours spent between 11AM and 4PM
                  .divTableRow
                    .divTableCell Childhood (up to age 10 yrs)
                    .divTableCell
                      input#Childhood_Period(type='number' name='exposure_childhood_period')
                      span.highlight
                      span.bar(style='')
                    .divTableCell
                      input#Childhood_Time(type='number' name='exposure_childhood_time')
                      span.highlight
                      span.bar(style='')
                  .divTableRow
                    .divTableCell Adolescence (11-18 yrs)
                    .divTableCell
                      input#Adolescence_Weeks(type='number' name='exposure_adolescence_period')
                      span.highlight
                      span.bar(style='')
                    .divTableCell
                      input#Adolescence_Time(type='number' name='exposure_adolescence_time')
                      span.highlight
                      span.bar(style='')
                  .divTableRow
                    .divTableCell Adulthood (&ge;18 yrs)
                    .divTableCell
                      input#Adulthood_Weeks(type='number' name='exposure_adulthood_period')
                      span.highlight
                      span.bar(style='')
                    .divTableCell
                      input#Adulthood_Time(type='number' name='exposure_adulthood_time')
                      span.highlight
                      span.bar(style='')
                  .divTableRow
                    .divTableCell(style='font-size:10px;')
                      | 10 years before melanoma diagnosis (for
                      | cases) or last 10 years
                      | (for controls)
                    .divTableCell
                      input#10_years_before_period(type='number' name='exposure_10_years_before_period')
                      span.highlight
                      span.bar(style='')
                    .divTableCell
                      input#10_years_before_time(type='number' name='exposure_10_years_before_time')
                      span.highlight
                      span.bar(style='')

            label Most recent intense intermittent sun exposure
            p(style='font-size:10px;') Provide date or month of last time of intense intermittent sun exposure before the diagnosis of melanoma (for cases) or before this interview (for controls)
            .form-group
              input#most_recent_date.datepicker.form-control(type='text' placeholder='Select date' required='' name='most_recent_sunburn')

            label Severe sunburns (Grade 2), lifetime before diagnosis.
            p(style='font-size:10px')
              | Definition: suburns with erythema and pain for more than 2 days or with blisters
              | (no matter for how long)
            .form-group
              .divTable(style='width: 100%;')
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder(style='width:34%;')
                      span At age &lt; 18 yrs ?
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      select#id_Sunburns_less18.form-control(name='sunburn_before_18')
                        option(value='No') No
                        option(value='Yes') Yes
                        option(value='NotKnow') Not know
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      .hidden-control
                        .group(style='margin-bottom:-4%;')
                          input#Sunburns_Yes_less18.special_on_div_table(type='number' name='number_sunburns_before_18')
                          span.highlight.special_on_div_table
                          span.bar.special_on_div_table
                          label Number:
            .form-group
              .divTable(style='width: 100%;')
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder(style='width:34%;')
                      span At age &gt;= 18 yrs ?
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      select#id_Sunburns_Yes_greater18.form-control(name='sunburn_after_18')
                        option(value='No') No
                        option(value='Yes') Yes
                        option(value='NotKnow') Not know
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      .hidden-control
                        .group(style='margin-bottom:-4%;')
                          input#Sunburns_Yes_greater18.special_on_div_table(type='number' name='number_sunburns_after_18')
                          span.highlight.special_on_div_table
                          span.bar.special_on_div_table
                          label Number:
            .form-group
              .divTable(style='width: 100%;')
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder(style='width:34%;')
                      span At site of melanoma ?
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      select#id_Site_melanoma.form-control(name='sunburn_melanoma_site')
                        option(value='No') No
                        option(value='Yes') Yes
                        option(value='NotKnow') Not know
                    .divTableCell.NoDivTableBorder(style='width:32%;')
            .form-group
              .divTable(style='width: 100%;')
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder(style='width:34%;')
                      span In the last 5 years ?
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      select#id_Sunburns_last5.form-control(name='sunburn_last5')
                        option(value='No') No
                        option(value='Yes') Yes
                        option(value='NotKnow') Not know
                    .divTableCell.NoDivTableBorder(style='width:32%;')
                      .group.hidden-control(style='margin-bottom:-4%;')
                        input#Sunburns_last5.special_on_div_table(type='number' name='number_sunburns_last5')
                        span.highlight
                        span.bar
                        label Number:

            legend(for='162128682') Sunscreen use (summer time)
            .divTable
              .divTableBody
                .divTableRow
                  .divTableCell Age Period
                  .divTableCell Summer Sunscreen
                  .divTableCell Summer Sunscreen
                .divTableRow
                  .divTableCell
                  .divTableCell Did you use sunscreen during summertime?
                  .divTableCell Type of sunscreen used
                .divTableRow
                  .divTableCell Childhood (up to age 10 yrs)
                  .divTableCell
                    select#id_SunExposure_childhood.form-control(name='childhood_sunscreen_frequency')
                      option(value='NoChoosed') Select sun exposure...
                      option(value='Never') Never
                      option(value='less50') time exposure &lt; 50%
                      option(value='greater50') time exposure &gt; 50%
                      option(value='always') always
                      option(value='notKnown') not known
                  .divTableCell
                    select#id_Sunscreen_childhood.form-control(name='childhood_sunscreen_type')
                      option(value='NoChoosed') Select sunscreen protection...
                      option(value='less20') SPF &lt; or = 20
                      option(value='greater20') SPF &gt; 20
                      option(value='notKnown') not known
                .divTableRow
                  .divTableCell Adolescence (11-18 yrs)
                  .divTableCell
                    select#id_SunExposure_adolescence.form-control(name='adolescence_sunscreen_frequency')
                      option(value='NoChoosed') Select sun exposure...
                      option(value='Never') Never
                      option(value='less50') time exposure &lt; 50%
                      option(value='greater50') time exposure &gt; 50%
                      option(value='always') always
                      option(value='notKnown') not known
                  .divTableCell
                    select#id_Sunscreen_adolescence.form-control(name='adolescence_sunscreen_type')
                      option(value='NoChoosed') Select sunscreen protection...
                      option(value='less20') SPF &lt; or = 20
                      option(value='greater20') SPF &gt; 20
                      option(value='notKnown') not known
                .divTableRow
                  .divTableCell Adulthood (&ge;18 yrs)
                  .divTableCell
                    select#id_SunExposure_adulthood.form-control(name='adulthood_sunscreen_frequency')
                      option(value='NoChoosed') Select sun exposure...
                      option(value='Never') Never
                      option(value='less50') time exposure &lt; 50%
                      option(value='greater50') time exposure &gt; 50%
                      option(value='always') always
                      option(value='notKnown') not known
                  .divTableCell
                    select#id_Sunscreen_adulthood.form-control(name='adulthood_sunscreen_type')
                      option(value='NoChoosed') Select sunscreen protection...
                      option(value='less20') SPF &lt; or = 20
                      option(value='greater20') SPF &gt; 20
                      option(value='notKnown') not known
                .divTableRow
                  .divTableCell(style='font-size:10px;')
                    | 10 years before melanoma diagnosis (for
                    | cases) or last 10 years
                    | (for controls)
                  .divTableCell
                    select#id_SunExposure_10years_before.form-control(name='last10_sunscreen_frequency')
                      option(value='NoChoosed') Select sun exposure...
                      option(value='Never') Never
                      option(value='less50') time exposure &lt; 50%
                      option(value='greater50') time exposure &gt; 50%
                      option(value='always') always
                      option(value='notKnown') not known
                  .divTableCell
                    select#id_Sunscreen_10years_before.form-control(name='last10_sunscreen_type')
                      option(value='NoChoosed') Select sunscreen protection...
                      option(value='less20') SPF &lt; or = 20
                      option(value='greater20') SPF &gt; 20
                      option(value='notKnown') not known

            label Sun protection other than sunscreen use (summertime over the last 10 yrs) (Optional)

            span Did you use a hat ?
            select#id_Hat.form-control(name='hat_frequency')
              option(value='NoChoosed') Select sun time exposure ...
              option(value='Never') Never
              option(value='less50') time exposure &lt; 50%
              option(value='greater50') time exposure &gt; 50%
              option(value='Always') Always
              option(value='Notknown') Not known

            span Did you use protective clothing ?
            select#id_Clothing.form-control(name='clothing_frequency')
              option(value='NoChoosed') Select sun time exposure ...
              option(value='Never') Never
              option(value='less50') time exposure &lt; 50%
              option(value='greater50') time exposure &gt; 50%
              option(value='Always') Always
              option(value='Notknown') Not known

            span Did you seek the shade or stay indoors during peak UVR hours?
            select#id_UVR.form-control(name='UVR_frequency')
              option(value='NoChoosed') Select sun time exposure ...
              option(value='Rarely') Rarely
              option(value='Sometimes') Sometimes
