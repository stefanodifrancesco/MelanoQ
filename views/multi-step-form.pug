extends layout

block append style
    link(rel='stylesheet' href='/stylesheet/multi-step-form.css')
    link(rel='stylesheet' href='/stylesheet/icheck.css')
    link(rel='stylesheet' href='/stylesheet/alt.css')
    link(rel='stylesheet' href='/stylesheet/modal.css')

block append script
    script(src='/javascript/MyScript.js')

block content
  div.profile-header
    button.questionnaires-list-button(onclick="var t = document.getElementById(\"msform\"); t.classList.add(\"fade-out-top\"); window.location.href = '/home';")
      |  QUESTIONNAIRES<br>
      i.fa.fa-caret-down

    button.new-questionnaire-button(onclick="window.location.href = '/stepform';")
      |  CREATE NEW<br>
      i.fa.fa-caret-down
          
    button.statistics-button(onclick="var t = document.getElementById(\"msform\"); t.classList.add(\"fade-out-top\"); window.location.href = '/statistics';")
      |  STATISTICS<br>
      i.fa.fa-caret-down

    button.btn.logout-button(onclick="window.location.href = '/logout';")
        |  Logout

  div.progressline
    // progressbar
    ul#progressbar
        li.active DEMOGRAPHIC
        li PHENOTYPE
        li ULTRAVIOLET EXPOSURE
        li LIFESTYLE HABITS
        li CLINICAL EXAMINATION
        li MEDICAL HISTORY
        li FAMILY HISTORY
        li MELANOMA CHARACTERISTICS

  div.form
    // multistep form
    form#msform.fade-in-top(action="/stepform" method="POST" autocomplete="off")
    
        fieldset
          div.scrollingdiv(id='scrollerdiv')
            h2.fs-title GENERAL
            h3.fs-subtitle Completed by phisician

            fieldset
              label 
                | Subject
              .form-group
                select.form-control(name='general[subject]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='' selected='') Select Subject ...
                  option(value='Case') Case
                  option(value='Control') Control
                span.error1
                  i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldsetCodeNumber
              label 
                | Database Code Number
              .divTable
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder
                      .form-group
                        select#DatabaseCodeCountry.form-control(name='general[code_country]' required='required' data-rule-required='true' data-msg-required='Please select the country')
                          option(value='' selected='') Select Country ...
                          option(value='01') Greece
                          option(value='02') Italy
                          option(value='03') Spain
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle

                    .divTableCell.NoDivTableBorder
                      .form-group
                        select#DatabaseCodeCenter.form-control(name='general[code_center]' disabled='disabled' required='required' data-rule-required='true' data-msg-required='Please select the center')
                          <option value="" selected>Select Center ...</option>
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle

                    .divTableCell.NoDivTableBorder
                      .form-group
                        select#DatabaseCodeType.form-control(name='general[code_type]' required='required' data-rule-required='true' data-msg-required='Select melanoma case')
                          option(value='' selected='') Select case type ...
                          option(value='M') Melanoma Case
                          option(value='C') Control subject
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle

              input#codeNumber(type='text' readonly='' name='general[code_number]')

            fieldset#fieldsetAdmin
              label 
                | Date of questionnaire administration and phenotyping
              .form-group
                input.datepicker.form-control(type='text' placeholder='Select date' name='general[survey_date]' required='required' data-rule-required='true' data-msg-required='Required field')
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
                
            fieldset#fieldsetTypeOfMelanoma
              label(for='type_of_melanoma') Type of melanoma
              .form-group
                select#Melanoma.form-control(name='general[melanoma_type]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='' selected='') Select Type ...
                  option(value='Sporadic') Sporadic
                  option(value='Familial') Familial
                  option(value="Don't know") Don't know
                  option(value='Other') Other (complex family history), specify:
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
              input#Other_Melanoma.hidden-control.inputMelanoma(type='text' name="general[other_melanoma_type]")


          input#confirmbutton.start(type='button' value='CONFIRM')

        fieldset
          div#handchange
            h2.fs-title Click start button and let the client fill in the questionnaire
            
          input#fillbutton.start(type='button' value='START')

        fieldset
          div.scrollingdiv(id='scrollerdiv')
            h2.fs-title DEMOGRAPHIC
            h3.fs-subtitle Completed by case/control

            fieldset#fieldsetGender
              label 
                | Sex
              .form-group
                select.form-control(name='demographic[gender]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='' selected='') Select Gender...
                  option(value='Female') Female
                  option(value='Male') Male
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
                    
            fieldset#fieldsetDateOfBirth
              .divTable
                .divTableBody
                  .divTableRow
                    .divTableCell.NoDivTableBorder
                      label Place of birth
                      .form-group
                        input.form-control(type='text' name="demographic[place_of_birth]" placeholder='Place' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                            i.error-log.fa.fa-exclamation-triangle
                    .divTableCell.NoDivTableBorder
                      label Birthday
                      .form-group
                        input.datepicker.form-control(type='text' name="demographic[birthday]" placeholder='Select date' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                            i.error-log.fa.fa-exclamation-triangle
                    .divTableCell.NoDivTableBorder
                      label Weight(Kg)
                      .form-group
                        input#weight_input.form-control(type='number' min='30' max='150' name='demographic[weight]' placeholder='Weight' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                            i.error-log.fa.fa-exclamation-triangle
                    .divTableCell.NoDivTableBorder
                      label Height(Cm)
                      .form-group
                        input#height_input.form-control(type='number' min='50' max='210' name='demographic[height]' placeholder='Height' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                            i.error-log.fa.fa-exclamation-triangle
                
            fieldset#fieldsetEthnicity
              label 
                | Ethnicity
              .form-group
                select#ethnicity_input.form-control(name='demographic[ethnicity]' required='required' data-rule-required='true' data-msg-required='Please include your ethnicity')
                  option(value='' selected='') Select Ethnicity ...
                  option(value='White/Europe') White/Europe
                  option(value='White/NorthAfrica') White/NorthAfrica
                  option(value='White/MiddleEast') White/MiddleEast
                  option(value='White/Jewish') White/Jewish
                  option(value='Black or African American') Black or African American
                  option(value='Asian') Asian
                  option(value='Hispanic or Latino (i.e. from South or Central America)')
                    | Hispanic or Latino (i.e.
                    | from
                    | South or Central America)
                  option(value='Other') Other (Specify)
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
              input#OtherEthnicity.hidden-control.inputEthnicity(type='text' name="demographic[other_ethnicity]" placeholder="Specify ethnicity")
              
            fieldset#fieldResidency
              label
                | Residency (multiple residency can be added)
              .form-group
                div(class="ui-widget")
                  input#searchBox(placeholder='Insert at least 2 characters')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
            .residency_date_class
            .json_residency
              
            fieldset#fieldsetEducation
              label 
                | Education
              .form-group
                select#education_input.form-control(name='demographic[education]' required='required' data-rule-required='true' data-msg-required='Please include your education')
                  option(value='' selected='') Select Range...
                  option(value='Up to junior high school (up to 14-16 yrs)')
                    | Up to junior high school (up to 14-16
                    | yrs)
                  option(value='High school (up to 18-19 yrs)') High school (up to 18-19 yrs)
                  option(value='University') University
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
                  
            fieldset#fieldsetOccupational
              label 
                | Current occupational status
              .form-group
                select#current_occupational_status.form-control(name='demographic[occupational_status]' required='required' data-rule-required='true' data-msg-required='Please include your status')
                  option(value='' selected='') Select Status...
                  option(value='Employed') Employed
                  option(value='Working at home') Working at home
                  option(value='Unemployed') Unemployed
                  option(value='Student') Student
                  option(value='Retired') Retired
                span.error1
                  i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldHistory
              label(for='History' style='font-size: 12px;')
                | History of occupation(optional History of occupation
                | (starting from most recent and
                | going backwards in time record duration from YYYY to YYYY)
              .form-group
                select#History.form-control
              .form-group.dynamic_ddl
              .form-group.div_date
              #json-table-wrapper-history.table

          div.alphaformfooter
          div.formfooter
          input.scroll-button-bottom(type='button')
          input#firstnext.next.action-button(type='button' name='next' value='Next')


        fieldset
          div.scrollingdiv
            h2.fs-title PHENOTYPE
            h3.fs-subtitle Completed by case/control

            label 1) Skin Type
            fieldset#fieldsetSkinResponse
              label
                | Which of the following sentences does better describe your skin response to sun
                | exposure at noon for 30 minutes in the first days of the summer without sunscreen/clothing protection?
              .form-group
                select#skin-response.form-control(name='phenotype[skin_response]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='') Select Skin Response ...
                  option(value='Not burn') Not burn
                  option(value='Burns lightly') Burns lightly
                  option(value='Burns moderately') Burns moderately
                  option(value='Burns severely') Burns severely
                span.error1
                  i.error-log.fa.fa-exclamation-triangle
                  
            fieldset#fieldSkin_tan
              label
                | Does your skin tan after a prolonged and repeated sun exposure (e.g. after
                | several
                | days as at the end of the summer) without protecting it with sunscreen or clothing?
              .form-group
                .divTable.divImgTable
                  .divTableBody
                    .divTableRow
                      .divTableCell
                        span No Tan
                      .divTableCell
                        span Tan Lightly
                      .divTableCell
                        span Tan Moderately
                      .divTableCell
                        span Tan Deeply
                    .divTableRow
                      .divTableCell
                        input#No_Tan(type='radio' name='phenotype[skin_tan]' value='No tan' required='required' data-rule-required='true' data-msg-required='Select exactly one')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Tan_Light(type='radio' name='phenotype[skin_tan]' value='Tans lightly')
                      .divTableCell
                        input#Tan_Moderate(type='radio' name='phenotype[skin_tan]' value='Tans moderately')
                      .divTableCell
                        input#Tan_Deep(type='radio' name='phenotype[skin_tan]' value='Tans deeply')
                    .divTableRow
                      .divTableCell
                        img(src='img/Skin-Tan/No_Tan_1.png' width='100' height='110')
                      .divTableCell
                        img(src='img/Skin-Tan/Light_Tan_1.png' width='100' height='110')
                      .divTableCell
                        img(src='img/Skin-Tan/Moderate_Tan_1.png' width='100' height='110')
                      .divTableCell
                        img(src='img/Skin-Tan/Deep_Tan_1.png' width='100' height='110')
                
            fieldset#fieldsetEye
              label(for='') 2) Eye Color
              .form-group
                .divTable.divImgTable
                  .divTableBody
                    .divTableRow
                      .divTableCell
                        span Light (green,blue,gray)
                      .divTableCell
                        span Medium (light brown, hazel)
                      .divTableCell
                        span Dark (dark brown, black)
                    .divTableRow
                      .divTableCell
                        input#Light_Eyes(type='radio' name='phenotype[eye_color]' value='Light' required='required' data-rule-required='true' data-msg-required='Select exactly one')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Medium_Eyes(type='radio' name='phenotype[eye_color]' value='Medium')
                      .divTableCell
                        input#Dark_Eyes(type='radio' name='phenotype[eye_color]' value='Dark')
                    .divTableRow
                      .divTableCell
                        img(src='img/Color-Eye/Light_eyes-removebg.png' width='150' height='75' style="margin-right: 10px;")
                      .divTableCell
                        img(src='img/Color-Eye/Medium_eyes-removebg.png' width='150' height='75' style="margin-left: 22px;")
                      .divTableCell
                        img(src='img/Color-Eye/Dark_eyes-removebg.png' width='120' height='75')

            fieldset#fieldsetHair
              label(for='1275694163') 3) Hair color (at age of 18 yrs)
              .form-group
                .divTable.divImgTable
                  .divTableBody
                    .divTableRow
                      .divTableCell
                        span Red Hair Color
                      .divTableCell
                        span Blonde Hair Color
                      .divTableCell
                        span Light Brown Color
                      .divTableCell
                        span Dark Brown Color
                      .divTableCell
                        span Dark Color
                    .divTableRow
                      .divTableCell
                        input#Red_Hair(type='radio' name='phenotype[hair_color]' value='Red' required='required' data-rule-required='true' data-msg-required='Select exactly one')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Blonde_Hair(type='radio' name='phenotype[hair_color]' value='Blonde')
                      .divTableCell
                        input#Light_brown_Hair(type='radio' name='phenotype[hair_color]' value='Light brown')
                      .divTableCell
                        input#Dark_brown_Hair(type='radio' name='phenotype[hair_color]' value='Dark brown')
                      .divTableCell
                        input#Dark_Hair(type='radio' name='phenotype[hair_color]' value='Black')
                    .divTableRow
                      .divTableCell
                        img(src='img/Color-hair/Red_hair-removebg.png' width='150' height='50')
                      .divTableCell
                        img(src='img/Color-hair/Blonde_hair-removebg.png' width='150' height='50')
                      .divTableCell
                        img(src='img/Color-hair/Light_brown_hair-removebg.png' width='100' height='50')
                      .divTableCell
                        img(src='img/Color-hair/Dark_brown_hair-removebg.png' width='150' height='50')
                      .divTableCell
                        img(src='img/Color-hair/Dark_hair-removebg.png' width='50' height='50')
              .form-group.hidden-control

            fieldset#fieldsetFreckles
              label(for='id_freckles')
                | 4) FrecklesDefinition: Light brown small macules since childhood, on the face, during
                | summer time (disappear in winter)
              .form-group
                .divTable.divImgTable
                  .divTableBody
                    .divTableRow
                      .divTableCell
                        span No Freckles
                      .divTableCell
                        span Few Freckles
                      .divTableCell
                        span Some Freckles
                      .divTableCell
                        span Many Freckles
                    .divTableRow
                      .divTableCell
                        input#No_Freckles(type='radio' name='phenotype[freckles]' value='None' required='required' data-rule-required='true' data-msg-required='Select exactly one')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Few_Freckles(type='radio' name='phenotype[freckles]' value='Few')
                      .divTableCell
                        input#Some_Freckles(type='radio' name='phenotype[freckles]' value='Some')
                      .divTableCell
                        input#Many_Freckles(type='radio' name='phenotype[freckles]' value='Many')
                    .divTableRow
                      .divTableCell
                        img(src='img/Freckles/No_freckles.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Freckles/Few_freckles.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Freckles/Some_freckles.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Freckles/Many_freckles.png' width='100' height='130')
                
            fieldset#fieldsetNevi
              label(for='id_Nevi') 5) Nevi frequency
              .form-group
                .divTable.divImgTable
                  .divTableBody
                    .divTableRow
                      .divTableCell
                        span None Nevi
                      .divTableCell
                        span Few Nevi
                      .divTableCell
                        span Some Nevi
                      .divTableCell
                        span Many Nevi
                    .divTableRow
                      .divTableCell
                        input#No_Nevi(type='radio' name='phenotype[nevi]' value='None' required='required' data-rule-required='true' data-msg-required='Select exactly one')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Few_Nevi(type='radio' name='phenotype[nevi]' value='Few')
                      .divTableCell
                        input#Some_Nevi(type='radio' name='phenotype[nevi]' value='Some')
                      .divTableCell
                        input#Many_Nevi(type='radio' name='phenotype[nevi]' value='Many')
                    .divTableRow
                      .divTableCell
                        img(src='img/Nevi/None_Nevi.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Nevi/Few_Nevi.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Nevi/Some_Nevi.png' width='100' height='130')
                      .divTableCell
                        img(src='img/Nevi/Many_Nevi.png' width='100' height='130')

          div.alphaformfooter
          div.formfooter
          input.scroll-button-bottom(type='button')
          input#firstprevious.previous.action-button(type='button' name='previous' value='Previous')
          input#secondnext.next.action-button(type='button' name='next' value='Next')

        fieldset
          div.scrollingdiv
            h2.fs-title HISTORY OF ULTRAVIOLET EXPOSURE
            h3.fs-subtitle Completed by case/control

            fieldset#fieldsetUltravioletExposure
              label
                | 6) Occupational sun exposure(i.e. outdoor occupation with at least 4 hours/day spent
                | outdoor in the sun)
              .form-group
                select#id_UltravioletExposure.form-control(name='ultraviolet[occupational_sun_exposure][presence]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='' selected='') Select...
                  option(value='No') No
                  option(value='Yes') Yes
                span.error1
                  i.error-log.fa.fa-exclamation-triangle

              .form-group.hidden-control(style='margin-top:20px;')
                .group
                  label Specify the occupation:
                  input#UltravioletExposure_Yes.form-control(type='text' name='ultraviolet[occupational_sun_exposure][occupation_name]' required='required' data-rule-required='true' data-msg-required='Required field')           
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/day?:
                  input#UltravioletExposure_hours_for_day.form-control(type='number' min='0' max='12' name='ultraviolet[occupational_sun_exposure][hours_day]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/month?:
                  input#UltravioletExposure_hours_for_month.form-control(type='number' min='0' max='360' name='ultraviolet[occupational_sun_exposure][hours_month]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/year?:
                  input#UltravioletExposure_hours_for_year.form-control(type='number' min='0' max='4520' name='ultraviolet[occupational_sun_exposure][hours_year]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many years ?:
                  input#UltravioletExposure_many_years.form-control(type='number' min='0' max='50' name='ultraviolet[occupational_sun_exposure][number_of_years]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                  
            fieldset#fieldsetRecreationalExposure
              label
                | 7) Recreational sun exposure (i.e. outdoor hobbies or physical activities–other than
                | sunbathing–with at least 4 hours/day spent outdoor in the sun)
              .form-group
                select#id_RecreationalExposure.form-control(name='ultraviolet[recreational_sun_exposure][presence]' required='required' data-rule-required='true' data-msg-required='Required field')
                  option(value='' selected='') Select...
                  option(value='No') No
                  option(value='Yes') Yes
                span.error1
                  i.error-log.fa.fa-exclamation-triangle

              .form-group.hidden-control(style='margin-top:20px;')
                .group
                  label Specify the occupation:
                  input#RecreationalExposure_Yes(type='text' name='ultraviolet[recreational_sun_exposure][occupation_name]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/day?:
                  input#Recreational_hours_for_day(type='number' min='0' max='12' name='ultraviolet[recreational_sun_exposure][hours_day]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/month?:
                  input#Recreational_hours_for_month(type='number' min='0' max='360' name='ultraviolet[recreational_sun_exposure][hours_month]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many hours/year?:
                  input#Recreational_hours_for_year(type='number' min='0' max='4520' name='ultraviolet[recreational_sun_exposure][hours_year]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle
                .group
                  label How many years ?:
                  input#Recreational_many_years(type='number' min='0' max='50' name='ultraviolet[recreational_sun_exposure][number_of_years]' required='required' data-rule-required='true' data-msg-required='Required field')
                  span.error1
                    i.error-log.fa.fa-exclamation-triangle

            fieldset#fieldsetIntermittent
              label
                | 8) Intermittent sun exposure (i.e. sun exposure in spring and summer time, including
                | vacation, during peak hours–11AM-4PM)
              .form-group
                .divTable
                  .divTableBody
                    .divTableRow
                      .divTableCell Age Period
                      .divTableCell Exposure Period
                      .divTableCell Exposure Time
                    .divTableRow
                      .divTableCell
                      .divTableCell Weeks of vacation
                      .divTableCell Hours spent between 11AM and 4PM
                    .divTableRow
                      .divTableCell Childhood (up to age 10 yrs)
                      .divTableCell
                        input#Childhood_Period(type='number' min='0' max='12' name='ultraviolet[exposure_childhood][period]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Childhood_Time(type='number' min='0' name='ultraviolet[exposure_childhood][time]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                    .divTableRow
                      .divTableCell Adolescence (11-18 yrs)
                      .divTableCell
                        input#Adolescence_Weeks(type='number' min='0' max='12' name='ultraviolet[exposure_adolescence][period]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Adolescence_Time(type='number' min='0' max='5' name='ultraviolet[exposure_adolescence][time]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                    .divTableRow
                      .divTableCell Adulthood (≥18 yrs)
                      .divTableCell
                        input#Adulthood_Weeks(type='number' min='0' max='12' name='ultraviolet[exposure_adulthood][period]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                      .divTableCell
                        input#Adulthood_Time(type='number' min='0' max='5' name='ultraviolet[exposure_adulthood][time]' required='required' data-rule-required='true' data-msg-required='Required field')
                        span.error1
                          i.error-log.fa.fa-exclamation-triangle
                    .divTableRow
                      .divTableCell(style='font-size:10px;')
                        | 10 years before melanoma diagnosis (for
                        | cases) or last 10 years
